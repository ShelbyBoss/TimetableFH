<UserControl
    x:Class="TimetableFH.AutoSuggestBoxContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TimetableFH"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400"
    x:Name="control">

    <AutoSuggestBox Margin="3" Text="{Binding ElementName=control,Path=Text,Mode=TwoWay}"
                    ItemsSource="{Binding ElementName=control,Path=ItemsSource,Mode=TwoWay}"
                    GotFocus="AutoSuggestBox_GotFocus" LostFocus="AutoSuggestBox_LostFocus">

    </AutoSuggestBox>
</UserControl>
